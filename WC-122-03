string generateString(int n, int m) 
{
    string s="";
    char ch='c';
    for(int i=0;i<n-m+2;i++)
    {
        if(i%2==0)
        {
            s+='a';
        }
        else 
        {
            s+='b';
        }
    }
    for(int i=n-m+2;i<n;i++)
    {
        s+=ch;
        ch++;
    }
    return s;
}
